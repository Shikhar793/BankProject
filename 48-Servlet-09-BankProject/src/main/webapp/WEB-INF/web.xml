<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <filter>
  	<filter-name>CustomerRegisterFilter</filter-name>
  	<display-name>CustomerRegisterFilter</display-name>
  	<description></description>
  	<filter-class>com.ay.filter.CustomerRegisterFilter</filter-class>
  </filter>
  <filter>
  	<filter-name>CustomerLoginFilter</filter-name>
  	<display-name>CustomerLoginFilter</display-name>
  	<description></description>
  	<filter-class>com.ay.filter.CustomerLoginFilter</filter-class>
  </filter>
  
  <filter-mapping>
  	<filter-name>CustomerRegisterFilter</filter-name>
  	<url-pattern>/cReg</url-pattern>
  </filter-mapping>
  <filter-mapping>
  	<filter-name>CustomerLoginFilter</filter-name>
  	<url-pattern>/cLogin</url-pattern>
  </filter-mapping>
  <servlet>
  	<servlet-name>CustomerRegisterServlet</servlet-name>
  	<display-name>CustomerRegisterServlet</display-name>
  	<description></description>
  	<servlet-class>com.ay.servlets.CustomerRegisterServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CustomerLoginServlet</servlet-name>
  	<display-name>CustomerLoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.ay.servlets.CustomerLoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SendMoneyServlet</servlet-name>
  	<display-name>SendMoneyServlet</display-name>
  	<description></description>
  	<servlet-class>com.ay.servlets.SendMoneyServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>TransactionHistoryServlet</servlet-name>
  	<display-name>TransactionHistoryServlet</display-name>
  	<description></description>
  	<servlet-class>com.ay.servlets.TransactionHistoryServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EditProfileServlet</servlet-name>
  	<display-name>EditProfileServlet</display-name>
  	<description></description>
  	<servlet-class>com.ay.servlets.EditProfileServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CustomerRegisterServlet</servlet-name>
  	<url-pattern>/cReg</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CustomerLoginServlet</servlet-name>
  	<url-pattern>/cLogin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SendMoneyServlet</servlet-name>
  	<url-pattern>/sendMoney</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TransactionHistoryServlet</servlet-name>
  	<url-pattern>/TxHis</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EditProfileServlet</servlet-name>
  	<url-pattern>/editProfile</url-pattern>
  </servlet-mapping>
</web-app>
